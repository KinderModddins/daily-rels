
--// Beautiful Visual Script GUI for Roblox //

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local TweenService = game:GetService("TweenService")
local UserInputService = game:GetService("UserInputService")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Player = Players.LocalPlayer
local Camera = workspace.CurrentCamera

-- GUI Setup
local screenGui = Instance.new("ScreenGui", game.CoreGui)
screenGui.Name = "VisualScriptGUI"
local draggable = false

local function createDraggableFrame()
	local frame = Instance.new("Frame")
	frame.Size = UDim2.new(0, 200, 0, 250)
	frame.Position = UDim2.new(0, 50, 0.5, -125)
	frame.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
	frame.BorderSizePixel = 0
	frame.Active = true
	frame.Draggable = true

	local corner = Instance.new("UICorner", frame)
	corner.CornerRadius = UDim.new(0, 12)
	frame.Parent = screenGui
	return frame
end

local mainFrame = createDraggableFrame()

-- Title
local title = Instance.new("TextLabel", mainFrame)
title.Text = "Script Menu"
title.Size = UDim2.new(1, 0, 0, 30)
title.BackgroundTransparency = 1
title.TextColor3 = Color3.fromRGB(255, 255, 255)
title.Font = Enum.Font.GothamBold
title.TextSize = 18

-- Notification UI
local function Notify(message)
	local notif = Instance.new("TextLabel", screenGui)
	notif.Size = UDim2.new(0, 250, 0, 40)
	notif.Position = UDim2.new(0.5, -125, 0.9, 0)
	notif.BackgroundTransparency = 0.2
	notif.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
	notif.TextColor3 = Color3.fromRGB(255, 255, 255)
	notif.Font = Enum.Font.Gotham
	notif.TextSize = 16
	notif.Text = message
	notif.AnchorPoint = Vector2.new(0.5, 0.5)

	local corner = Instance.new("UICorner", notif)
	corner.CornerRadius = UDim.new(0, 8)

	local tweenIn = TweenService:Create(notif, TweenInfo.new(0.5), {Position = UDim2.new(0.5, -125, 0.85, 0)})
	tweenIn:Play()
	wait(3)
	local tweenOut = TweenService:Create(notif, TweenInfo.new(0.5), {Position = UDim2.new(0.5, -125, 1.2, 0)})
	tweenOut:Play()
	tweenOut.Completed:Wait()
	notif:Destroy()
end

-- Create a button function
local function createButton(text, posY, callback)
	local button = Instance.new("TextButton", mainFrame)
	button.Text = text
	button.Size = UDim2.new(0.9, 0, 0, 30)
	button.Position = UDim2.new(0.05, 0, 0, posY)
	button.BackgroundColor3 = Color3.fromRGB(55, 55, 55)
	button.TextColor3 = Color3.fromRGB(255, 255, 255)
	button.Font = Enum.Font.Gotham
	button.TextSize = 14
	button.BorderSizePixel = 0

	local corner = Instance.new("UICorner", button)
	corner.CornerRadius = UDim.new(0, 6)

	button.MouseEnter:Connect(function()
		button.BackgroundColor3 = Color3.fromRGB(75, 75, 75)
	end)
	button.MouseLeave:Connect(function()
		button.BackgroundColor3 = Color3.fromRGB(55, 55, 55)
	end)

	button.MouseButton1Click:Connect(callback)
end

-- Kill Aura
local killAuraEnabled = false
createButton("Toggle Kill Aura", 40, function()
	killAuraEnabled = not killAuraEnabled
	Notify("Kill Aura: " .. (killAuraEnabled and "ON" or "OFF"))
	while killAuraEnabled do
		for _, v in pairs(workspace:GetDescendants()) do
			if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Humanoid.Health > 0 then
				local distance = (Player.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude
				if distance < 50 then
					v.Humanoid.Health = 0
				end
			end
		end
		task.wait(0.3)
	end
end)

-- Magic Bullets
local magicBulletsEnabled = false
createButton("Toggle Magic Bullets", 80, function()
	magicBulletsEnabled = not magicBulletsEnabled
	Notify("Magic Bullets: " .. (magicBulletsEnabled and "ON" or "OFF"))
	while magicBulletsEnabled do
		local closest, dist = nil, math.huge
		for _, v in pairs(workspace:GetDescendants()) do
			if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Humanoid.Health > 0 then
				local d = (Player.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude
				if d < dist then
					dist = d
					closest = v
				end
			end
		end
		if closest then
			local args = {[1] = closest.HumanoidRootPart.Position, [2] = closest.HumanoidRootPart}
			ReplicatedStorage:FindFirstChild("RemoteEvents"):FindFirstChild("ShootEvent"):FireServer(unpack(args))
		end
		task.wait(0.2)
	end
end)

-- WallHack
local wallHackEnabled = false
createButton("Toggle WallHack", 120, function()
	wallHackEnabled = not wallHackEnabled
	Notify("WallHack: " .. (wallHackEnabled and "ON" or "OFF"))
	for _, v in pairs(workspace:GetDescendants()) do
		if v:IsA("BasePart") then
			if wallHackEnabled then
				local esp = Instance.new("Highlight", v)
				esp.FillColor = Color3.fromRGB(255, 0, 0)
				esp.OutlineColor = Color3.fromRGB(255, 255, 255)
			else
				if v:FindFirstChildOfClass("Highlight") then
					v:FindFirstChildOfClass("Highlight"):Destroy()
				end
			end
		end
	end
end)

-- TP To End
createButton("TP To End", 160, function()
	local finish = workspace:FindFirstChild("EndZone") or workspace:FindFirstChild("Finish")
	if finish then
		Player.Character:MoveTo(finish.Position + Vector3.new(0, 5, 0))
		Notify("Teleported to end!")
	else
		Notify("Finish not found!")
	end
end)

-- Noclip + Free Camera
local noclipEnabled = false
createButton("Toggle Noclip + FreeCam", 200, function()
	noclipEnabled = not noclipEnabled
	Notify("Noclip + FreeCam: " .. (noclipEnabled and "ON" or "OFF"))
	if noclipEnabled then
		RunService.Stepped:Connect(function()
			if Player.Character then
				for _, part in ipairs(Player.Character:GetDescendants()) do
					if part:IsA("BasePart") then
						part.CanCollide = false
					end
				end
			end
		end)
	end
end)

-- Timer (security reminder)
task.delay(600, function()
	Notify("10 минут истекли! Проверьте безопасность использования.")
	local highlight = TweenService:Create(mainFrame, TweenInfo.new(0.5, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {
		BackgroundColor3 = Color3.fromRGB(200, 50, 50)
	})
	highlight:Play()
end)

